
class Fib {

  static int fib(int n)
  {
     if (n <= 2)
       return 1;
     return fib(n-1) + fib(n-2);
  }

  static void printFibs(int n)
  {
    int i = 1;
    while (i <= n) {
      IO.putString ("fib(");
      IO.putInt (i);
      IO.putString (") = ");
      IO.putInt (fib(i));
      IO.putString ("\n");
      i = i + 1;
    }
  }
  
  public static void main(String[] argv)
  {
    printFibs(20);
  }
}
