#!/usr/bin/env decaf

class Adder {
  static public void main (String[] argv)
  {
    IO.putString ("Enter two integers: \n");
    int x = IO.getInt ();
    int y = IO.getInt ();

    IO.putInt (x);
    IO.putString (" + ");
    IO.putInt (y);
    IO.putString (" = ");
    IO.putInt (x+y);
    IO.putString ("\n");
  }
}