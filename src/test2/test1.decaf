#!/usr/bin/env decaf

// Inputs ints and prints :) until a 0 is entered

class Aaa {
    public static void main (String[] argv) {
        IO.putString("Enter ints, 0 to exit.");
        endl();
        while (IO.getInt() != 0) {
            IO.putString(":)");
            endl();
        }
        IO.putString(":(");
        endl();
    }

    void endl() { IO.putChar('\n'); }
}