#!/usr/bin/env decaf

// mutable variable tests

class Aaa {
    public static void main (String[] argv) {
        IO.putString("Start the countdown at: ");
        int x = IO.getInt();

        while (x > 0) {
            IO.putInt(x);
            IO.putChar('\n');
            x = x-1;
        }
        IO.putString("Happy New Year!\n");
    }
}