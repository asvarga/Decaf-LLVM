#!/usr/bin/env decaf

// mutable variable tests

class Aaa {
    // public static void main (String[] argv) {
    public static void main () {
        IO.putString("Start the countdown at: ");
        int x = IO.getInt();
        endl();

        while (x > 0) {        // TODO: while
            IO.putInt(x);
            IO.putChar('\n');
            x = x-1;
        }
        IO.putString("Happy New Year!");
        endl();
    }

    void endl() { IO.putChar('\n'); }
}